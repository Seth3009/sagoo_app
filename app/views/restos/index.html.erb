<div class="container">
  <nav class="navbar navbar-inverse">
    <p class="navbar-title">Daftar Lokasi Resto</p>
  </nav>
</div><br>
<div class="container">
  <div class="well">
    <div class="row">
      <div class="col-lg-12">
        <table>
          <thead>
            <tr>
              <th>Started</th>
              <th>Ended</th>
              <th>Employee</th>
              <th>Position group</th>
              <th>Location</th>
              <th colspan="3">Action</th>
            </tr>
          </thead>
        
          <tbody>
            <% @restos.each do |resto| %>
              <tr>
                <td><%= resto.started %></td>
                <td><%= resto.ended %></td>
                <td><%= resto.employee.name %></td>
                <td><%= resto.position_group.name %></td>
                
                <td><%= link_to 'Show', resto %></td>
                <td><%= link_to 'Edit', edit_resto_path(resto) %></td>
                <td><%= link_to 'Destroy', resto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <% end %>
          </tbody>
        </table>

       </div>
    </div>
  </div>
</div>